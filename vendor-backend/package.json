{
  "name": "vercel-function-elysia-demo",
  "module": "api/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run server.ts",
    "start": "bun run server.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun run src/seed.ts",
    "db:seed:preprod": "bun run src/seed-preprod.ts",
    "db:reset": "bun run db:push && bun run db:seed"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.31.6"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@elysiajs/swagger": "^1.3.1",
    "@neondatabase/serverless": "^1.0.2",
    "drizzle-orm": "^0.44.7",
    "elysia": "^1.3.15"
  }
}
